{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "es6": true
  },
  "globals": {},
  "rules": {
    "no-bitwise": 2,
    "curly": [2, "all"],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "new-cap": 0,
    "no-caller": 2,
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-irregular-whitespace": 2,
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "no-cond-assign": [2, "except-parens"],
    "no-implicit-coercion": [
      2,
      {
        "boolean": false,
        "string": true,
        "number": true
      }
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-multiple-empty-lines": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "dot-location": [2, "property"],
    "operator-linebreak": [2, "after"],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "no-spaced-func": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "array-bracket-spacing": [2, "never", {}],
    "space-in-parens": [2, "never"],
    "no-trailing-spaces": 2,
    "yoda": [2, "never"],
    "comma-style": [2, "last"],
    "dot-notation": 2,
    "eol-last": 2,
    "one-var": [0, "never"],
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "else": {
            "before": true
          },
          "while": {
            "before": true
          },
          "catch": {
            "before": true
          }
        }
      }
    ],
    "space-before-blocks": [2, "always"]
  }
}
